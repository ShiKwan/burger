<h1>SK Burger Stand!</h1>

<h2>Serving</h2>

<ul>
    {{#each burgers}} 
        <li>{{burger}}</li>
    {{/each}}
</ul>


<h2>New Order</h2>
<form class="create-form">

    <div class="form-group">
        <label for="bn">Burger Name:</label>
        <input type="text" id="bn" name="name">
    </div>
    <button type="submit">Serve Burger</button>
</form>